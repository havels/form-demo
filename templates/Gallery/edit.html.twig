{% extends 'layout.html.twig' %}

{% block content %}
    <h1>Edit gallery: {{ gallery.title }}</h1>

    {{ form_start(form) }}
        {{ form_row(form.title) }}
        {{ form_row(form.items, {
            label: false,
        }) }}
        <button type="submit" class="btn btn-primary"><i class="glyphicon glyphicon-floppy-save"></i> Save</button>
    {{ form_end(form) }}
{% endblock %}
